generator client {
  provider = "prisma-client"
  output   = "../src/app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model HashAddress { 
  id    Int     @id @default(autoincrement()) 
  hash String
  netCrypto String
  createdAt DateTime @default(now())
} 

model IpGeo {
  id       Int      @id @default(autoincrement())
  ip       String
  country  String
  region   String
  city     String
  createdAt DateTime @default(now())
}